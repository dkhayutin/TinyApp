<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <header>
   <%- include ../views/partials/_header.ejs %>
 </header>

  <ul>
    <% for(key in urls) {  %>
      <form  method="POST"  action="/urls/<%= key %>/delete">
        <li><%= key %> - <%= urls[key] %> <button type="submit" name="delete">Delete</button></li>
        <a href="/urls/<%= key %>">Edit</a>
    </form>
  <% } %>
  </ul>
  <a href="/urls/new">Shorten Your Link Here!</a>


</body>
</html>
